{
   "version": 2,
   "name": "Tennis Khelo Backend",
   "headers": [
      {
        "key": "access-control-allow-origin",
        "value": "*"
      }
    ],
   "builds": [
     { 
       "src": "./index.js", 
       "use": "@vercel/node",
       "config": {
         "maxDuration":150,
         "memory": 3009
       }
     }
   ],
   "routes": [
     { "src": "/(.*)", "dest": "/" }
   ] }

   

