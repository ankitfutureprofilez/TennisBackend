{
   "version": 2,
   "name": "Tennis Khelo Backend",
   "builds": [
     { 
       "src": "./index.js", 
       "use": "@vercel/node",
       "config": {
         "maxDuration":60,
         "memory": 3009
       }
     }
   ],
   "routes": [
     { "src": "/(.*)", "dest": "/" }
   ],
   "functions": {
      "controllers/*.js": { "maxDuration": 60 },
      "middleware/**/*.js": { "maxDuration": 60 }
    }
 }


