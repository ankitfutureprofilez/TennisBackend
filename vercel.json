{
   "version": 2,
   "name": "Tennis Khelo Backend",
   "builds": [
     { 
       "src": "./index.js", 
       "use": "@vercel/node",
       "config": {
         "maxDuration": 30,
         "memory": 3009
       }
     }
   ],
   "routes": [
     { "src": "/(.*)", "dest": "/" }
   ],
   "rewrites": [
     { "source": "/(.*)", "destination": "/" }
   ],
   "headers": [
     {
       "source": "/api/(.*)"
      
     }
   ],
   "cleanUrls": true,
   "trailingSlash": true
 }
 